<div style="margin-top: 2em;">
  <div>
    <div class="confusionMatrixHeader confusionMatrixHeaderTop">
      Classe Real
    </div>
    <div class="d-flex flex-row">
      <div class="confusionMatrixHeader confusionMatrixHeaderLeft">
        Classe Estimada
      </div>
      <table class="table table-bordered" style="margin: 0;">

          <tr>
            <td class="confusionMatrixLabelHeader"></td>
            <% for(var i=0; i < results.labels.length ; i++) { %>
              <td class="confusionMatrixLabelHeader">
                <%= results.labels[i] %>
              </td>
              <% } %>
          </tr>

          <% for(var i=0; i < results.labels.length; i++) { %>
            <tr>
              <td class="confusionMatrixLabelHeader">
                <%= results.labels[i] %>
              </td>
              <% for(var j=0; j < results.labels.length ; j++) { %>
                <td>
                  <%= (!!results.confusionMatrix && !!results.confusionMatrix.matrix ) ?
                      results.confusionMatrix.matrix[i][j] : "" %>
                </td>
                <% } %>
            </tr>
            <% } %>
      </table>
    </div>
  </div>
</div>